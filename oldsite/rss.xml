<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:rss="http://www.sitemaps.org/schemas/sitemap/0.9">
<channel>
  <title>deomorxsy's old blog</title>
    <link>https://deomorxsy.github.io</link>
    <description>Linux • FOSS • Virtualization</description>
    <generator>Jekyll</generator>
    <language>en</language>

    {% for page in site.pages %}
    <url>
        <loc>{{page.url}}</loc>
        <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.5</priority>
    </url>
    {% endfor %}

    {% for post in site.posts %}
    <url>
        <loc>{{post.url}}</loc>
        <lastmod>{{site.time | date: '%Y-%m-%d' }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.5</priority>
    </url>
    {% endfor %}
    </channel>
</urlset>
